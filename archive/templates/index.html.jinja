{% extends "layout.html.jinja" %}

{% block content %}
  <div class="jumbotron" id="header">
    <div class="container">
      <div class="media">


        <div class="media-left media-top">
          <img class="media-object" src="{{ url_for('static', filename='images/cassette.png') }}" alt="" />
        </div>

        <div class="media-body">
          <h1 class="media-heading">Theme<br>Time<br>Radio Tapes</h1>
        </div>

      </div>
      <p>Autogenerated <img id="spotifyLogo" align="middle" src="{{ url_for('static', filename='images/spotify/Spotify_Logo_CMYK_Green.png') }}" alt="" /> playlists
        based on tunes from Bob Dylan's radio show</p>

      <div class="row">
        <div class="col-md-5">
        </div>
        <div class="col-md-7">
        </div>
      </div>

    </div>
  </div>

  <div id="features_placeholder"></div>

{% endblock %}


{% block style %}
  <link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='css/index.css') }}">
{% endblock %}


{% block script %}
  <script type="text/javascript" src="{{ url_for('static', filename='js/index.js') }}"></script>
{% endblock %}
